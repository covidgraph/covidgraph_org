{
	
	const self = $.this;

	const neoConnector = $.call('getCovidGraphConnector');

	const query = 'MATCH (patent:Patent) ' +
		'WHERE ID(patent) = ' + self.covidGraphId + ' ' +
		'RETURN patent';

	
	const results = neoConnector.query({query : query});
	
	if(results.length !== 1) {
		$.log('Cardinality Mismatch for Patent ', self.id);
		return {};
	}
	
	const patent = $.first(results);
	return patent;

}