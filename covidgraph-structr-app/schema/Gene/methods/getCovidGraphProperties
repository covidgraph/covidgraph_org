{
	
	const self = $.this;

	const neoConnector = $.call('getCovidGraphConnector');

	const query = 'MATCH (gene:Gene) ' +
		'WHERE ID(gene) = ' + self.covidGraphId + ' ' +
		'RETURN gene';

	
	const results = neoConnector.query({query : query});
	
	if(results.length !== 1) {
		$.log('Cardinality Mismatch for Gene.getCovidGraphProperties(', self.id, ')');
		$.log('Query: ', query);
		return {};
	}

	const gene = $.first(results);
	return gene;

}