{

	const self = $.this;

	$.log('START getRelatedPapers: ', self.id);
	
	const listGenes = self.getGenes();
	let hashmap = {};

	for (let gene of listGenes) 
	{
		if(!gene.paperLookUp)
			gene.getPapers();

		if(gene.score < 100)
		{
			let genePapers = gene.getPapers();
			for(let paper of genePapers)
			{	
				if($.empty(hashmap[paper.id]))
					hashmap[paper.id] = [gene];
				else
					hashmap[paper.id].push(gene);
			}
		}	
	}

	let sortable = [];
	for (let paper in hashmap) {
		let key   = paper;
		let value = hashmap[paper];
		let pair  = [key, value];
    	sortable.push(pair);
	}

	sortable.sort(function(a, b) {
		const sizeA = a[1].length;
		const sizeB = b[1].length;
		if(sizeA > sizeB)        return -1;
		else if(sizeA === sizeB) return 0;
		else                     return 1;
	});

	
	let topfivePapers = sortable.filter(paper=>paper[0]!=self.id).slice(0,25);
	
	$.log(JSON.stringify(topfivePapers));
	
	$.log('END getRelatedPapers: ', self.id);
	
	return topfivePapers;
	
}