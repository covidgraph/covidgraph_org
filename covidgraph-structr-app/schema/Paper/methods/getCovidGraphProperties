{
	
	const self = $.this;

	const neoConnector = $.call('getCovidGraphConnector');

	const query = 'MATCH (paper:Paper) ' +
		'WHERE ID(paper) = ' + self.covidGraphId + ' ' +
		'RETURN paper';

	
	const results = neoConnector.query({query : query});
	
	if(results.length !== 1) {
		$.log('Cardinality Mismatch for Paper ', self.id);
		return {};
	}
	
	const paper = $.first(results);
	return paper;

}